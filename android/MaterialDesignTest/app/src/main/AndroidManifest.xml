<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.testexample.materialdesigntest">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:usesCleartextTraffic="true"
        android:name=".CRS"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseMaterialTheme"
        tools:targetApi="m">
        <activity android:name=".ui.examination.ExamDrawer">
        </activity>
        <activity android:name=".ui.instructions.InstructionActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.collegeDashboard.CollegeDashboard"
            android:exported="true">
        </activity>
        <activity android:name=".ui.login.CollegeLoginActivity" />
        <activity android:name=".ui.result.ResultActivity" />
        <activity android:name=".ui.resetAuthentication.ResetAuthenticationActivity"/>

        <activity android:name=".ui.studentdashboard.StudentDashboardActivity" />
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>