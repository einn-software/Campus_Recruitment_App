<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testexample.materialdesigntest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BaseMaterialTheme">
        <activity android:name=".ui.instructions.InstructionActivity">

        </activity>
        <activity android:name=".ui.collegedashboard.CollegeDashboard" />
        <activity android:name=".ui.login.CollegeLoginActivity" />
        <activity android:name=".ui.result.ResultActivity" />
        <activity android:name=".ui.examination.ExaminationActivity">

        </activity>
        <activity android:name=".ui.resetAuthentication.ResetAuthenticationActivity" />
        <activity android:name=".ui.studentdashboard.StudentDashboardActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>